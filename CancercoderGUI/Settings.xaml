<Window x:Class="CancercoderGUI.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CancercoderGUI"
        mc:Ignorable="d"
        Title="Settings" Height="350" Width="600" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="btnSave" Content="Save" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="434,291,0,0" Click="btnSave_Click"/>
        <StackPanel HorizontalAlignment="Left" Height="128" VerticalAlignment="Top" Width="574" Margin="10,10,0,0">
            <Label x:Name="label" Content="Temp Folder" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.883,-0.332"/>
            <TextBox x:Name="textTempFolder" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" Margin="10,0,0,0"/>
        </StackPanel>
    </Grid>
</Window>
